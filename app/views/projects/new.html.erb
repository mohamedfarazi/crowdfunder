<%= form_for @project do |f|%>
		<p><%= f.label :name %></br>
		<%= f.text_field :name %></p>
	<p><%= f.label :goal_in_cents %></br>
		<%= f.number_field :goal_in_cents %></p>
		<p><%= f.label :start_date %></br>
		<%= f.datetime_select:start_date %></p>
		<p><%= f.label :end_date %></br>
		<%= f.datetime_select :end_date %></p>
		<p><%= f.label :description %></br>
		<%= f.text_area :description %></p>
		<p><%= f.label :image %></br>
		<%= f.text_field :image %></p>

		<%= f.submit "Next" %>
<% end %>